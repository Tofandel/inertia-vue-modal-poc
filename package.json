{
  "name": "@tofandel/inertia-vue-modal",
  "version": "0.2.26",
  "private": false,
  "description": "Load any inertia route into a modal",
  "author": "Adrien Foulon",
  "scripts": {
    "build": "vue-cli-service build --target lib resources/js/index.ts && tsc --emitDeclarationOnly",
    "lint": "vue-cli-service lint resources",
    "lint:fix": "vue-cli-service lint . --fix",
    "prepublishOnly": "npm run build"
  },
  "main": "dist/inertia-vue-modal.umd.min.js",
  "types": "types/index.d.ts",
  "files": [
    "dist/*",
    "resources/*",
    "types/*"
  ],
  "dependencies": {
    "@inertiajs/inertia": "^0.11.0",
    "@inertiajs/inertia-vue3": "^0.6.0",
    "vue": "^3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-plugin-typescript": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.13.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^8.6.0",
    "typescript": "^4.6.3"
  },
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/tofandel/inertia-vue-modal-poc.git"
  }
}
